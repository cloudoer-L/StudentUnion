<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
        "http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
    <package name="dormitory" namespace="/dormitory" extends="struts-default">
        <action name="indexUI" class="dormitoryAction" method="indexUI">
            <result name="success">/WEB-INF/views/basics/dormitory/index.jsp</result>
        </action>
        <action name="addUI" class="dormitoryAction" method="addUI">
            <result name="success">/WEB-INF/views/basics/dormitory/add.jsp</result>
        </action>
        <action name="importUI" class="dormitoryAction" method="importUI">
            <result name="success">/WEB-INF/views/basics/dormitory/import.jsp</result>
        </action>
        <action name="infoUI" class="dormitoryAction" method="infoUI">
            <result name="success">/WEB-INF/views/basics/dormitory/info.jsp</result>
        </action>

        <action name="getAll" class="dormitoryAction" method="getAll"></action>
        <action name="getByPage" class="dormitoryAction" method="getByPage"></action>
        <action name="getById" class="dormitoryAction" method="getById"></action>
        <action name="getByNumber" class="dormitoryAction" method="getByNumber"></action>
        <action name="getStudents" class="dormitoryAction" method="getStudents"></action>
        <action name="add" class="dormitoryAction" method="add"></action>
        <action name="update" class="dormitoryAction" method="update"></action>
        <action name="delete" class="dormitoryAction" method="delete"></action>
        <action name="importFile" class="dormitoryAction" method="importFile"></action>
        <action name="exportFile" class="dormitoryAction" method="exportFile"></action>
        <action name="appointAdmin" class="dormitoryAction" method="appointAdmin"></action>
    </package>
</struts>