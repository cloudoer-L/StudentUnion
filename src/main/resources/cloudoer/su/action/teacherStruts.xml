<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
        "http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
    <package name="teacher" namespace="/teacher" extends="struts-default">
        <action name="indexUI" class="teacherAction" method="indexUI">
            <result name="success">/WEB-INF/views/basics/teacher/index.jsp</result>
        </action>
        <action name="addUI" class="teacherAction" method="addUI">
            <result name="success">/WEB-INF/views/basics/teacher/add.jsp</result>
        </action>
        <action name="importUI" class="teacherAction" method="importUI">
            <result name="success">/WEB-INF/views/basics/teacher/import.jsp</result>
        </action>

        <action name="getAll" class="teacherAction" method="getAll"></action>
        <action name="getByPage" class="teacherAction" method="getByPage"></action>
        <action name="getById" class="teacherAction" method="getById"></action>
        <action name="getByNumber" class="teacherAction" method="getByNumber"></action>
        <action name="add" class="teacherAction" method="add"></action>
        <action name="update" class="teacherAction" method="update"></action>
        <action name="delete" class="teacherAction" method="delete"></action>
        <action name="importFile" class="teacherAction" method="importFile"></action>
        <action name="exportFile" class="teacherAction" method="exportFile"></action>
    </package>
</struts>