<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
        "http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
    <package name="member" namespace="/member" extends="struts-default">
        <action name="indexUI" class="memberAction" method="indexUI">
            <result name="success">/WEB-INF/views/basics/member/index.jsp</result>
        </action>
        <action name="addUI" class="memberAction" method="addUI">
            <result name="success">/WEB-INF/views/basics/member/add.jsp</result>
        </action>
        <action name="importUI" class="memberAction" method="importUI">
            <result name="success">/WEB-INF/views/basics/member/import.jsp</result>
        </action>

        <action name="getAll" class="memberAction" method="getAll"></action>
        <action name="getByPage" class="memberAction" method="getByPage"></action>
        <action name="getById" class="memberAction" method="getById"></action>
        <action name="getByNumber" class="memberAction" method="getByNumber"></action>
        <action name="add" class="memberAction" method="add"></action>
        <action name="update" class="memberAction" method="update"></action>
        <action name="delete" class="memberAction" method="delete"></action>
        <action name="importFile" class="memberAction" method="importFile"></action>
        <action name="exportFile" class="memberAction" method="exportFile"></action>
    </package>
</struts>