<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
        "http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
    <package name="department" namespace="/department" extends="struts-default">
        <action name="indexUI" class="departmentAction" method="indexUI">
            <result name="success">/WEB-INF/views/basics/department/index.jsp</result>
        </action>
        <action name="addUI" class="departmentAction" method="addUI">
            <result name="success">/WEB-INF/views/basics/department/add.jsp</result>
        </action>
        <action name="importUI" class="departmentAction" method="importUI">
            <result name="success">/WEB-INF/views/basics/department/import.jsp</result>
        </action>

        <action name="getAll" class="departmentAction" method="getAll"></action>
        <action name="getByPage" class="departmentAction" method="getByPage"></action>
        <action name="getById" class="departmentAction" method="getById"></action>
        <action name="getByNumber" class="departmentAction" method="getByNumber"></action>
        <action name="add" class="departmentAction" method="add"></action>
        <action name="update" class="departmentAction" method="update"></action>
        <action name="delete" class="departmentAction" method="delete"></action>
        <action name="importFile" class="departmentAction" method="importFile"></action>
        <action name="exportFile" class="departmentAction" method="exportFile"></action>
        <action name="getMembers" class="departmentAction" method="getMembers"></action>
    </package>
</struts>