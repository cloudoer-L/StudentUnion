<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
        "http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
    <package name="position" namespace="/position" extends="struts-default">
        <action name="indexUI" class="positionAction" method="indexUI">
            <result name="success">/WEB-INF/views/basics/position/index.jsp</result>
        </action>
        <action name="addUI" class="positionAction" method="addUI">
            <result name="success">/WEB-INF/views/basics/position/add.jsp</result>
        </action>
        <action name="importUI" class="positionAction" method="importUI">
            <result name="success">/WEB-INF/views/basics/position/import.jsp</result>
        </action>

        <action name="getAll" class="positionAction" method="getAll"></action>
        <action name="getByPage" class="positionAction" method="getByPage"></action>
        <action name="getById" class="positionAction" method="getById"></action>
        <action name="getByNumber" class="positionAction" method="getByNumber"></action>
        <action name="add" class="positionAction" method="add"></action>
        <action name="update" class="positionAction" method="update"></action>
        <action name="delete" class="positionAction" method="delete"></action>
        <action name="importFile" class="positionAction" method="importFile"></action>
        <action name="exportFile" class="positionAction" method="exportFile"></action>
    </package>
</struts>